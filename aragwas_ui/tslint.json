{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "double"
        ],
        "indent": [
            true
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        // Pending fix for shorthand property names.
        "object-literal-sort-keys": false,
        "no-string-literal": false,

        "member-access": false
    },
    "rulesDirectory": []
}
